<%= form_for @taco do |f| %>
  Meat: <%= f.text_field :meat %><br />
  Zest : <%= f.number_field  :zest %><br />
  Spict : <%= f.number_field  :spicy %><br />
  Truck: <%= f.collection_select :truck_id, @trucks, :id, :name %><br />
 
<%= f.submit %>

  <% if flash[:notice] %>
    <% flash[:notice].each do | msg| -%>
      <%= content_tag :div, msg %>
    <% end -%>
  <% end %>
<% end %>